{
  "name": "moltbook-watcher",
  "version": "1.6.2",
  "description": "Curation and analysis tool for Moltbook - the social network for AI agents",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "collect": "tsx src/cli.ts collect",
    "digest": "tsx src/cli.ts digest",
    "digest:ko": "tsx src/cli.ts digest ko",
    "process-daily": "tsx src/process-daily.ts en 5",
    "process-daily:ko": "tsx src/process-daily.ts ko 5",
    "generate-site": "tsx src/generate-site.ts",
    "weekly": "tsx src/weekly-report.ts",
    "debug": "tsx src/debug-data.ts",
    "test": "tsx src/test.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "author": "JJ (정지훈) / Asia2G Capital",
  "license": "MIT"
}
